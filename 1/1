#include<stdio.h>
void mergearray(int,int*,int,int*,int*)
int main()
{
	int s1;

	printf("Enter arr1 size:");
	scanf("%d",&s1);
	int arr[s1];
	for(int i=0;i<s1;i++)
	{
		scanf("%d",&arr[i]);
	}

	int s2; 
	printf("Enter arr2 size:");
	scanf("%d",&s1);

	int arr2[s2]
		for(int i=0;i<s2;i++)
		{   
			scanf("%d",&arr2[i]);
		}

	int merge;
	merge=s1+s2;
	int arrmerge[merge];

	mergearray(s1,arr[],s2,arr2[],arrmerge[]);

}

void(int s1, int arr[],int s2, int arr2[], int arrmerge[])
{
	for(int i=0;i<s1;i++)
	{
		mergearr[i]=arr[1];
	}
	for(i=0;i<s2;i++)
	{
		mergearr[i]=arr[1];
	}

}
